{
    "name": "mbs/mbs-bundle",
    "description": "Symfony Bundle for Mbs",
    "type": "symfony-bundle",
    "license": "GPLv3",
    "authors": [
        {
            "name": "vilsafur",
            "email": "vilsafur@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mbs\\MbsBundle\\": ""
        }
    },
    "require": {
        "symfony/http-kernel": "^5.0",
        "symfony/console": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/config": "^5.0",
        "symfony/flex": "^1.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "symfony/phpunit-bridge": "^5.0"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
